{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "quotes": ["error", "double"],
        "linebreak-style": ["off", "windows"],
        "no-underscore-dangle": ["off"],
        "class-methods-use-this": "off",
        "consistent-return": "off",
        "no-console": "off",
        "no-unused-vars": "off"
    }
}
